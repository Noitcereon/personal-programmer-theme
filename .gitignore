# ignore everything in these folders.
wp-admin/*
wp-includes/*
wp-content/*

# unignore the themes folder in wp-content
!wp-content/themes

# ignore specific themes
wp-content/themes/twenty*/
wp-content/themes/generatepress

# ignore node dependency directories
node_modules/

# ignore log files and databases
*.log
*.sql
*.sqlite

# ignore any files beginning with wp- and ending with .php
wp-*.php

# any additional files to ignore
pwa-manifest.json
pwa-register-sw.js
pwa-sw.js
readme.html
xmlrpc.php
.htaccess
.htaccess.bk
default.php
index.php
license.txt
